{
  "verbose": true,
  "clearMocks": true,
  "transform": {
    ".*": "babel-jest"
  },
  "moduleFileExtensions": [
    "js"
  ],
  "moduleDirectories": [
    "src",
    "node_modules"
  ],
  "modulePaths": [
    "src"
  ],
  "collectCoverageFrom": [
    "src/**/*.{js,jsx}",
    "test/**/*.{js,jsx}",
    "!**/node_modules/**"
  ],
  "coverageDirectory": "./coverage",
  "notify": false,
  "roots": [
    "src/",
    "test/"
  ],
  "setupFiles": [
    "<rootDir>/test/setup.js"
  ],
  "coverageThreshold": {
     "global": {
       "branches": 5,
       "functions": 5,
       "lines": 5,
       "statements": 5
     }
   },
